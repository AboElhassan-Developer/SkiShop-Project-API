  "components": {
    "schemas": {
      "Address": {
        "required": [
          "city",
          "country",
          "line1",
          "postalCode",
          "state"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "line1": {
            "type": "string",
            "nullable": true
          },
          "line2": {
            "type": "string",
            "nullable": true
          },
          "city": {
            "type": "string",
            "nullable": true
          },
          "state": {
            "type": "string",
            "nullable": true
          },
          "postalCode": {
            "type": "string",
            "nullable": true
          },
          "country": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "AddressDto": {
        "required": [
          "city",
          "country",
          "line1",
          "postalCode",
          "state"
        ],
        "type": "object",
        "properties": {
          "line1": {
            "minLength": 1,
            "type": "string"
          },
          "line2": {
            "type": "string",
            "nullable": true
          },
          "city": {
            "minLength": 1,
            "type": "string"
          },
          "state": {
            "minLength": 1,
            "type": "string"
          },
          "postalCode": {
            "minLength": 1,
            "type": "string"
          },
          "country": {
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CartItem": {
        "required": [
          "brand",
          "pictureUrl",
          "productName",
          "type"
        ],
        "type": "object",
        "properties": {
          "productId": {
            "type": "integer",
            "format": "int32"
          },
          "productName": {
            "type": "string",
            "nullable": true
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "pictureUrl": {
            "type": "string",
            "nullable": true
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          },
          "brand": {
            "type": "string",
            "nullable": true
          },
          "type": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "CreateOrderDto": {
        "required": [
          "cartId",
          "deliveryMethodId",
          "paymentSummary",
          "shippingAddress"
        ],
        "type": "object",
        "properties": {
          "cartId": {
            "minLength": 1,
            "type": "string"
          },
          "deliveryMethodId": {
            "type": "integer",
            "format": "int32"
          },
          "shippingAddress": {
            "$ref": "#/components/schemas/ShippingAddress"
          },
          "paymentSummary": {
            "$ref": "#/components/schemas/PaymentSummary"
          }
        },
        "additionalProperties": false
      },
      "CreateProductDto": {
        "required": [
          "brand",
          "description",
          "name",
          "pictureUrl",
          "type"
        ],
        "type": "object",
        "properties": {
          "name": {
            "minLength": 1,
